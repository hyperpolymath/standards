@prefix void: <http://rdfs.org/ns/void#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix palimpsest: <https://palimpsest.license/vocab#> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix dcat: <http://www.w3.org/ns/dcat#> .

# ============================================================================
# VoID Description of Palimpsest License Dataset
# ============================================================================

<https://palimpsest.license/dataset>
    a void:Dataset ;
    dcterms:title "Palimpsest License Metadata Dataset"@en ;
    dcterms:title "Palimpsest Licentie Metadata Dataset"@nl ;
    dcterms:description """
        A comprehensive RDF dataset containing metadata about works licensed under
        the Palimpsest License, including emotional lineage, derivative works,
        lineage chains, and violation reports. This dataset supports the Palimpsest
        License's mission to protect creative works in the age of AI through
        quantum-proof traceability and symbolic attribution.
    """@en ;
    dcterms:creator <https://palimpsest.license/council> ;
    dcterms:publisher <https://palimpsest.license/council> ;
    dcterms:license <https://palimpsest.license/v0.4> ;
    dcterms:created "2024-01-15"^^xsd:date ;
    dcterms:modified "2025-11-22"^^xsd:date ;
    foaf:homepage <https://palimpsest.license> ;

    # Dataset statistics
    void:triples "0"^^xsd:integer ;  # To be updated with actual count
    void:entities "0"^^xsd:integer ;
    void:distinctSubjects "0"^^xsd:integer ;
    void:distinctObjects "0"^^xsd:integer ;
    void:properties "0"^^xsd:integer ;

    # Access methods
    void:sparqlEndpoint <https://sparql.palimpsest.license/query> ;
    void:uriSpace "https://palimpsest.license/" ;

    # Data dumps
    void:dataDump <https://palimpsest.license/data/dump.nt.gz> ;
    void:dataDump <https://palimpsest.license/data/dump.ttl.gz> ;
    void:dataDump <https://palimpsest.license/data/dump.rdf.gz> ;

    # Vocabulary and ontology
    void:vocabulary <http://purl.org/dc/terms/> ;
    void:vocabulary <http://xmlns.com/foaf/0.1/> ;
    void:vocabulary <http://www.w3.org/ns/prov#> ;
    void:vocabulary <https://palimpsest.license/vocab#> ;

    # Dataset partitions
    void:subset <https://palimpsest.license/dataset/works> ;
    void:subset <https://palimpsest.license/dataset/derivatives> ;
    void:subset <https://palimpsest.license/dataset/lineages> ;
    void:subset <https://palimpsest.license/dataset/violations> ;

    # Linksets to external datasets
    void:subset <https://palimpsest.license/linkset/orcid> ;
    void:subset <https://palimpsest.license/linkset/doi> ;
    void:subset <https://palimpsest.license/linkset/creative-commons> ;

    # Technical details
    void:feature <http://www.w3.org/ns/formats/Turtle> ;
    void:feature <http://www.w3.org/ns/formats/RDF_XML> ;
    void:feature <http://www.w3.org/ns/formats/N-Triples> ;
    void:feature <http://www.w3.org/ns/formats/JSON-LD> ;

    # Root resource
    void:rootResource <https://palimpsest.license/v0.4> ;

    # Example resources
    void:exampleResource <https://palimpsest.license/works/example-001> ;
    void:exampleResource <https://palimpsest.license/derivatives/example-001> ;

    # URI patterns
    void:uriRegexPattern "^https://palimpsest\\.license/(works|derivatives|lineages|violations)/[a-z0-9-]+$" ;

    # DCAT integration
    dcat:landingPage <https://palimpsest.license> ;
    dcat:keyword "license"@en, "AI ethics"@en, "emotional lineage"@en, "cultural heritage"@en ;
    dcat:theme <http://publications.europa.eu/resource/authority/data-theme/TECH> .

# ============================================================================
# Dataset Partitions
# ============================================================================

# Works partition
<https://palimpsest.license/dataset/works>
    a void:Dataset ;
    dcterms:title "Creative Works"@en ;
    dcterms:description "Original creative works licensed under the Palimpsest License"@en ;
    void:subset <https://palimpsest.license/dataset> ;
    void:class palimpsest:Work ;
    void:triples "0"^^xsd:integer ;
    void:entities "0"^^xsd:integer ;
    void:exampleResource <https://palimpsest.license/works/example-001> ;
    void:uriRegexPattern "^https://palimpsest\\.license/works/[a-z0-9-]+$" .

# Derivatives partition
<https://palimpsest.license/dataset/derivatives>
    a void:Dataset ;
    dcterms:title "Derivative Works"@en ;
    dcterms:description "Derivative works, remixes, and AI-generated content"@en ;
    void:subset <https://palimpsest.license/dataset> ;
    void:class palimpsest:Derivative ;
    void:triples "0"^^xsd:integer ;
    void:entities "0"^^xsd:integer ;
    void:exampleResource <https://palimpsest.license/derivatives/example-001> ;
    void:uriRegexPattern "^https://palimpsest\\.license/derivatives/[a-z0-9-]+$" .

# Lineages partition
<https://palimpsest.license/dataset/lineages>
    a void:Dataset ;
    dcterms:title "Lineage Chains"@en ;
    dcterms:description "Chain-of-custody tracking for creative works"@en ;
    void:subset <https://palimpsest.license/dataset> ;
    void:class palimpsest:Lineage ;
    void:triples "0"^^xsd:integer ;
    void:entities "0"^^xsd:integer ;
    void:exampleResource <https://palimpsest.license/lineages/example-001> ;
    void:uriRegexPattern "^https://palimpsest\\.license/lineages/[a-z0-9-]+$" .

# Violations partition
<https://palimpsest.license/dataset/violations>
    a void:Dataset ;
    dcterms:title "Violation Reports"@en ;
    dcterms:description "License violation reports and dispute resolutions"@en ;
    void:subset <https://palimpsest.license/dataset> ;
    void:class palimpsest:Violation ;
    void:triples "0"^^xsd:integer ;
    void:entities "0"^^xsd:integer ;
    void:exampleResource <https://palimpsest.license/violations/example-001> ;
    void:uriRegexPattern "^https://palimpsest\\.license/violations/[a-z0-9-]+$" .

# ============================================================================
# Linksets to External Datasets
# ============================================================================

# ORCID linkset
<https://palimpsest.license/linkset/orcid>
    a void:Linkset ;
    dcterms:title "Links to ORCID"@en ;
    dcterms:description "Links between Palimpsest creators and their ORCID profiles"@en ;
    void:linkPredicate foaf:account ;
    void:subjectsTarget <https://palimpsest.license/dataset/works> ;
    void:objectsTarget <https://orcid.org/> ;
    void:triples "0"^^xsd:integer .

# DOI linkset
<https://palimpsest.license/linkset/doi>
    a void:Linkset ;
    dcterms:title "Links to DOI"@en ;
    dcterms:description "Links between Palimpsest works and their DOI identifiers"@en ;
    void:linkPredicate dcterms:identifier ;
    void:subjectsTarget <https://palimpsest.license/dataset/works> ;
    void:objectsTarget <https://doi.org/> ;
    void:triples "0"^^xsd:integer .

# Creative Commons linkset
<https://palimpsest.license/linkset/creative-commons>
    a void:Linkset ;
    dcterms:title "Links to Creative Commons"@en ;
    dcterms:description "Related Creative Commons licenses and concepts"@en ;
    void:linkPredicate dcterms:relation ;
    void:subjectsTarget <https://palimpsest.license/dataset> ;
    void:objectsTarget <https://creativecommons.org/> ;
    void:triples "0"^^xsd:integer .

# ============================================================================
# Palimpsest Stewardship Council
# ============================================================================

<https://palimpsest.license/council>
    a foaf:Organization ;
    foaf:name "Palimpsest Stewardship Council"@en ;
    foaf:homepage <https://palimpsest.license> ;
    dcterms:description """
        A governance body of 7 members responsible for the Palimpsest License:
        3 Creator Representatives, 2 Legal Experts (IP, Dutch, or Scottish law),
        1 Technologist (AI ethics/decentralised systems), and 1 Cultural Heritage Advocate.
    """@en .

# ============================================================================
# Dataset Publisher Information
# ============================================================================

<https://palimpsest.license>
    a foaf:Document ;
    dcterms:title "Palimpsest License"@en ;
    dcterms:publisher <https://palimpsest.license/council> ;
    foaf:primaryTopic <https://palimpsest.license/v0.4> .

# ============================================================================
# SPARQL Endpoint Description
# ============================================================================

<https://sparql.palimpsest.license/query>
    a void:sparqlEndpoint ;
    dcterms:title "Palimpsest SPARQL Endpoint"@en ;
    dcterms:description "SPARQL query endpoint for Palimpsest License metadata"@en ;
    void:feature <http://www.w3.org/ns/sparql-service-description#SPARQL11Query> ;
    void:feature <http://www.w3.org/ns/sparql-service-description#SPARQL11Update> .

