{
    "name": "palimpsest/license-php",
    "description": "PHP library for Palimpsest License integration",
    "type": "library",
    "license": "Palimpsest-0.4",
    "keywords": ["palimpsest", "license", "copyright", "ai-ethics", "php"],
    "homepage": "https://palimpsestlicense.org",
    "authors": [
        {
            "name": "Palimpsest License Contributors",
            "email": "hello@palimpsestlicense.org"
        }
    ],
    "require": {
        "php": ">=7.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "squizlabs/php_codesniffer": "^3.7"
    },
    "autoload": {
        "psr-4": {
            "Palimpsest\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Palimpsest\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "lint": "phpcs src/"
    }
}
